      "tags": ["CourseSections"],
      "operationId": "courseSectionsGet",
      "parameters": [
        {
  "name" : "department_id",
  "in" : "query",
  "description" : "Get the course sections within a department.",
  "required" : true,
  "type" : "number",
  "format" : "integer"
}
        
      ],
